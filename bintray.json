{
  "package": {
    "name": "tcptester",
    "repo": "public",
    "subject": "rkbalgi",
    "userOrg": "daalitoy",
    "website_url": "${project.url}",
    "issue_tracker_url": "${project.url}/issues",
    "vcs_url": "${project.url}",
    "licenses": [
      "Apache-2.0"
    ],
    "labels": [
      "tcptester",
      "tcp-client",
      "tcp-loadtest"
