  "files": [
    {
      "includePattern": "pom.xml",
      "uploadPattern": "com/github/rkbalgi/tcptester/0.1.0/tcptester-0.1.0.pom"
    },
    {
      "includePattern": "target/(.*\.jar)",
      "uploadPattern": "com/github/rkbalgi/tcptester/0.1.0/$1"
    }
  ],
  "publish": true
}
