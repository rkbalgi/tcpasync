{
  "package": {
    "name": "tcptester",
    "repo": "public",
    "subject": "daalitoy",
    "website_url": "${project.url}",
    "issue_tracker_url": "${project.url}/issues",
    "vcs_url": "${project.url}",
    "licenses": [
      "Apache-2.0"
    ],
    "labels": [
      "tcptester",
      "tcp-client",
      "tcp-loadtest"
    ],
    "public_download_numbers": true,
    "public_stats": true
  },
  "version": {
    "name": "${project.version}",
    "released": "${maven.build.timestamp}",
    "vcs_tag": "${project.version}"
  },
  "files": [
    {
      "includePattern": "pom.xml",
      "uploadPattern": "com/github/rkbalgi/tcptester/${project.version}/tcptester-${project.version}.pom"
    },
    {
      "includePattern": "target/(.*\.jar)",
      "uploadPattern": "com/github/rkbalgi/tcptester/${project.version}/$1"
    }
  ],
  "publish": true
}
